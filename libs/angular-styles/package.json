{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "@tabler/angular-styles",
  "version": "0.7.3",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "ngPackage": {
    "copyPaths": [
      {
        "from": "libs/angular-styles/src/**/*.*",
        "to": "dist/libs/angular-styles/"
      }
    ],
    "lib": {
      "entryFile": "index.ts"
    },
    "dest": "../../dist/libs/angular-styles"
  },
  "peerDependencies": {
    "bootstrap": "^4.0.0",
    "font-awesome": "^4.7.0"
  }
}
